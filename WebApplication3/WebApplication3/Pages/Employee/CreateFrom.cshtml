@page
@model WebApplication3.Pages.Employee.CreateFromModel
@{
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<div class="col-md-4">
    <form method="post">
        <kendo-textbox name="textbox"
                       value="John Doe">
        </kendo-textbox>



        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="employeeBase.model1.Name" class="control-label"></label>
            <input asp-for="employeeBase.model1.Name" class="form-control" />
            <span asp-validation-for="employeeBase.model1.Name" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="employeeBase.model1.Email" class="control-label"></label>
            <input asp-for="employeeBase.model1.Email" class="form-control" />
            <span asp-validation-for="employeeBase.model1.Email" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="employeeBase.model1.Adderss" class="control-label"></label>
            <input asp-for="employeeBase.model1.Adderss" class="form-control" />
            <span asp-validation-for="employeeBase.model1.Adderss" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="employeeBase.model1.Date" class="control-label"></label>
            <input asp-for="employeeBase.model1.Date" class="form-control" />
            <span asp-validation-for="employeeBase.model1.Date" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="employeeBase.model2.TestName" class="control-label"></label>
            <input asp-for="employeeBase.model2.TestName" class="form-control" />

        </div>

        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
    </form>
</div>




